{
  "name": "LumenNexus API Dev Environment",
  "dockerComposeFile": ["docker-compose.yml"],
  "service": "app",
  "workspaceFolder": "/workspace",
  "postCreateCommand": "pip install -r requirements.txt",
  "shutdownAction": "none",
  "extensions": [
    "ms-python.python",
    "ms-azuretools.vscode-docker"
  ],
  "forwardPorts": [8000]
}